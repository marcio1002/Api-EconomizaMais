{
    "name": "marcio1002/ecomais",
    "description": "Um crud para manipulação de dados do EcoMais",
    "minimum-stability": "dev",
    "license": "proprietary",
    "version": "v1.1.11",
    "keywords": [
        "Query",
        "dados",
        "crud",
        "EcoMais"
    ],
    "homepage": "https://github.com/marcio1002/CrudEcoMais",
    "type": "package",
    "authors": [
        {
            "name": "Marcio Alemão",
            "email": "marcioalemao190@gmail.com",
            "homepage": "https://github.com/marcio1002",
            "role": "Developer"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    },
    "autoload": {
        "psr-4": {
            "Interfaces\\": "interfaces",
            "Server\\": "server/",
            "Model\\": "model/",
            "Controller\\": "controller/",
            "Web\\": "web/" 
        },
        "files": [
            "config.php"
        ]
    },
    "repositories": {
        "packagist.org": false,
        "private-packagist": {
            "type": "composer",
            "url": "https://repo.packagist.com/ecomais/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "coffeecode/router": "1.0.*",
        "phpmailer/phpmailer": "6.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
